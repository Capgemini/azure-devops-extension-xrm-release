  "tags": [
    "Dynamics",
    "Release",
    "Dynamics 365",
    "CRM",
    "CI",
    "CD",
    "Solution",
    "Import",
    "PackageDeployer",
    "Xrm"
  ],
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "files": [
