  "description": "Tasks for releasing into and managing Dynamics 365 instances.",
  "publisher": "capgemini-uk",
  "public": false,
  "icons": {
    "default": "logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
